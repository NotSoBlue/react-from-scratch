{
  "name": "react-from-scratch",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "webpack-dev-server -d --hot",
    "format:prettier": "prettier --write '**/*.{css,gql,graphql,html,js,jsx,json,less,md,mdx,scss,ts,tsx,vue,yaml,yml}' '.editorconfig' 'LICENSE'",
    "lint:styles": "stylelint --cache ./src/",
    "lint:scripts": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src/",
    "typecheck": "tsc --noEmit",
    "lint": "lint:scripts && npm run lint:styles",
    "fix:js": "lint:scripts && npm run lint:styles",
    "sort:imports": "import-sort --write",
    "pre-commit": "typecheck && npm run lint:scripts && npm run lint:styles"
  },
  "importSort": {
    ".js, .jsx": {
      "parser": "babylon",
      "style": "module"
    },
    ".ts, .tsx": {
      "parser": "typescript",
      "style": "module"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc --noEmit && lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "**/*.{css,gql,graphql,html,json,less,md,mdx,scss,vue,yaml,yml}": [
        "prettier --write",
        "git add"
      ],
      "**/*.{js,jsx,ts,tsx}": [
        "import-sort --write",
        "prettier --write",
        "eslint --cache --ext .js,.jsx,.ts,.tsx --fix",
        "git add",
        "jest --bail --findRelatedTests"
      ]
    },
    "concurrent": false
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@types/jest": "^24.0.11",
    "@types/styled-theming": "^2.2.0",
    "@typescript-eslint/eslint-plugin": "^1.4.2",
    "@typescript-eslint/parser": "^1.4.2",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-styled-components": "^1.10.0",
    "css-loader": "^2.1.0",
    "eslint": "^5.15.1",
    "eslint-config-airbnb-typescript": "^1.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-formatter-pretty": "^2.1.1",
    "eslint-plugin-eslint-comments": "^3.1.1",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.3.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-unicorn": "^7.1.0",
    "file-loader": "^3.0.1",
    "husky": "^1.3.1",
    "image-loader": "^0.0.1",
    "import-sort": "^6.0.0",
    "import-sort-cli": "^6.0.0",
    "import-sort-parser-babylon": "^6.0.0",
    "import-sort-parser-typescript": "^6.0.0",
    "import-sort-style-module": "^6.0.0",
    "jest": "^24.4.0",
    "jest-styled-components": "6.3.1",
    "lint-staged": "^8.1.5",
    "node-sass": "^4.11.0",
    "prettier": "^1.16.4",
    "prettier-package-json": "^2.1.0",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "stylelint": "^9.10.1",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.5.2",
    "ts-essentials": "^2.0.2",
    "ts-jest": "^24.0.0",
    "tslint": "^5.13.0",
    "tslint-react": "^3.6.0",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1"
  },
  "dependencies": {
    "@types/react": "^16.8.5",
    "@types/react-dom": "^16.8.2",
    "@types/styled-components": "^4.1.11",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "styled-components": "^4.1.3",
    "styled-theming": "^2.2.0",
    "typescript": "^3.3.3333"
  }
}
